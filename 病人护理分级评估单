<script type="text/javascript">
var basePath = "<%=basePath%>" ;
 function  lastDeal(){
	var arr = [$("#col011"),$("#col012"),$("#col013"),$("#col014"),$("#col015"),$("#col016")]; 
	var $eleArr = [$("#col191"),$("#col192"),$("#col193"),$("#col194"),$("#col195"),$("#col196")]; 
	var $eleArr2 = [$("#col197"),$("#col198"),$("#col199"),$("#col200"),$("#col201"),$("#col202")]; 
	var $eleArrAll = new Array();
	
	for(var i = 0 ;i < $eleArr2.length; i++){
		var $eleArrAll001 = new Array();
		 $eleArrAll001[0]=$eleArr2[i];
		 
		 
		for(var j = 0; j < 3;j++){
			var id = $eleArr2[i].attr("id");
			 id = id.substring(3);
			 id = Number(id);
			id+= (6*(j+1));
			var col="";
		 if(id<=9){
			 col="col00"+id;
		 }
		 if(id<=99 && id > 9){
			 col="col0"+ id;
		 }
		 if(id > 99){
			 col="col"+id;
		 }
			$eleArrAll001[j+1] = $("#"+col);
			
		}
		 $eleArrAll[i] = $eleArrAll001;
		 
	}
	
	//31  30
	for(var i = 0 ;i < arr.length; i++){
		var arrTem = new Array();
		arrTem[0]=arr[i];
		for(var j = 0; j <29;j++){
			 var id = arr[i].attr("id");
		 id = id.substring(3);
		 id = Number(id);
		 id+= (6*(j+1));
		 var col="";
		 if(id<=9){
			 col="col00"+id;
		 }
		 if(id<=99 && id > 9){
			 col="col0"+ id;
		 }
		 if(id > 99){
			 col="col"+id;
		 }
		 arrTem[j+1]=$("#"+col);
		 
		}
		
		for(var j = 0 ; j < arrTem.length ; j ++){
			arrTem[j].data("data",arrTem);
			arrTem[j].data("sumEle",$eleArr[i]);
			arrTem[j].data("checkArr",$eleArrAll[i]);
			arrTem[j].blur(function(){
				var sum =0;
				var arr = $(this).data("data");
				for(var k = 0 ; k < arr.length;k++ ){
					var  num = Number(arr[k].val());
					sum+=num;
				}
				var val001 = $(this).data("sumEle");
				val001.val(sum);
				var checkArr = $(this).data("checkArr");
				for(var l = 0 ; l < checkArr.length; l++){
					checkArr[l].prop("checked",false);
				}
				if( sum<= 40 ){
					
					checkArr[0].prop("checked",true);
					
				}
				if(sum > 40 && sum <=60){
					checkArr[1].prop("checked",true);
				}
				if(sum > 60 && sum< 100){
					checkArr[2].prop("checked",true);
				}
				if( sum == 100){
					checkArr[3].prop("checked",true);
				}
				
			});
		}
	}
	
	
 
 
 }
 function setPrintMode(LODOP){
		LODOP.SET_PRINT_MODE("PRINT_DUPLEX",2);
	}
 function ckPrintBack(){
	 var LODOP=getLodop(document.getElementById('LODOP'),document.getElementById('LODOP_EM'));
		var strStyleCSS="<link href='"+basePath+"css/printView.css' type='text/css' rel='stylesheet'>";
		var strBodyStyle = document.getElementById("ownCss") ;
		if (strBodyStyle != null) {
			strStyleCSS= strStyleCSS + "<style>"+strBodyStyle.innerHTML+"</style>";
		}
		//做某些事
		try{
			printExtend_s();
		}catch (e) {
			
		}
		try{
			setPrintMode(LODOP);
		}catch (e) {
			
		}
		var mainDoc =document.getElementById("fu");
		if(mainDoc ==null){
				return;
			}
		var outerHTM = 	mainDoc.outerHTML;
		var $textarea = $("#main").find("textarea");
		var $select = $("#main").find("select");
		if($("#justForTemp").length == 0){
			$("body").append($("<div id=\"justForTemp\"></div>"));
		}
		$("#justForTemp").html(outerHTM);
		$("#justForTemp").find("textarea").each(function(index,ele){
			$(this).replaceWith($textarea.eq(index).val());
			
		});
		var $ele = $("#justForTemp").find("div[id^='Bs_LimitedTextarea']").parent("div");
		if($ele.length > 0){
			$ele.remove();
		}
		$("#justForTemp").find("select").each(function(index,ele){
			$(this).replaceWith($select.eq(index).find("option:selected").text());
			
		});
		var htm = $("#justForTemp")[0].innerHTML;
		$("#justForTemp").remove();
		var mainDocContent=strStyleCSS+htm;
		LODOP.PRINT_INITA(0,0,"210mm","297mm","");
		
		try{
			//水印
			watermark(LODOP);
		}catch(e){
		}
		
		try{
		marginTop(LODOP,mainDocContent);
		}catch(e){
			LODOP.ADD_PRINT_HTM("0%","0%","100%","100%",mainDocContent);
		}
		var hengZong=1;
		try{
		kk_0130();
		hengZong=2;
		}catch(e){
		}
		LODOP.SET_PRINT_PAGESIZE(hengZong,0,0,"A4");
		//LODOP.PREVIEW();
	    LODOP.PRINT();//直接打印
 }
 function print(LODOP){
	 LODOP.PRINT();
 } 
 function setRightWidth(LODOP,html){
		LODOP.ADD_PRINT_HTM("0","0%","100%","100%",html);
	}
</script> 
